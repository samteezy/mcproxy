  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-context-proxy": "dist/cli.js"
  },
  "author": "Sam Taylor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samteezy/mcp-context-proxy.git"
  },
